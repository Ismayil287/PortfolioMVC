@model web8.Models.Entity.crudMVC
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!DOCTYPE html>
<html>
<head>
    <link rel="shortcut icon" href="~/images/favicon.ico" type="image/x-icon" />
    <link rel="apple-touch-icon" href="~/images/apple-touch-icon.png">
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/style.css">
    <link rel="stylesheet" href="~/css/responsive.css">
    <link rel="stylesheet" href="~/css/custom.css">
    <script src="~/js/modernizr.js"></script>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <meta name="viewport" content="width=device-width" />
    <title>New OR Update</title>
    <link href="~/RegistrationStyle.css" rel="stylesheet" />
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css">

</head>
<body>


    @using (Html.BeginForm("Create", "Home", FormMethod.Post))
    {
        <div class="panel shadow1">
            <form action="/Home/Create" method="post" class="login-form">
                <h1 class="animated fadeInUp animate1" id="title-login">Add New Project or Update Existing One</h1>
                <fieldset id="login-fieldset">
                    <div class="login animated fadeInUp animate2">
                        @Html.LabelFor(m => m.ProjectName)
                        @Html.TextBoxFor(m => m.ProjectName, new { @class = "" })
                    </div>
                    <div class="login animated fadeInUp animate3">
                        @Html.LabelFor(m => m.Owner)
                        @Html.TextBoxFor(m => m.Owner, new { @class = "" })
                    </div>
                </fieldset>
                @Html.HiddenFor(m => m.ID)
                <button type="submit" id="login-form-submit" class="login_form button animated fadeInUp animate4">Update</button>
            </form>
        </div>
    }

    <a href="#" id="scroll-to-top" class="dmtop global-radius"><i class="fa fa-angle-up"></i></a>
    <script src="~/js/all.js"></script>
    <script src="~/js/jquery.mobile.customized.min.js"></script>
    <script src="~/js/jquery.easing.1.3.js"></script>
    <script src="~/js/parallaxie.js"></script>
    <script src="~/js/slick.min.js"></script>
    <script src="~/js/animated-slider.js"></script>
    <script src="~/js/jqBootstrapValidation.js"></script>
    <script src="~/js/contact_me.js"></script>
    <script src="~/js/custom.js"></script>
</body>

</html>